{#
/**
 * @file
 * Default theme implementation for a Culturefeed agenda search result count.
 *
 * Available variables:
 * - summary: The result count summary
 *
 * @see template_preprocess_culturefeed_agenda_search_result_count_summary()
 *
 * @ingroup themeable
 */
#}

{%
  set classes = [
    'culturefeed-agenda-search-result-count-summary',
  ]
%}

<div{{ attributes.addClass(classes) }}>
  {% if summary %}
    {{ summary }}
  {% endif %}
</div>
